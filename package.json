{
  "name": "lander",
  "version": "0.0.1",
  "description": "A VDOM engine made for the modern web",
  "main": "dist/lander.js",
  "module": "dist/lander.es.js",
  "browser": "dist/lander.browser.js",
  "scripts": {
    "build": "rollup -c && uglifyjs ./dist/lander.browser.js -c unsafe=true,unsafe_comps=true,unsafe_math=true,passes=3 -m --source-map -o ./dist/lander.browser.min.js",
    "start": "parcel src/index.html -p 8080 --out-dir build --public-url / --no-source-maps --no-cache",
    "test": "npm run test:jest && npm run test:bundlesize",
    "clean": "git clean -fdX .",
    "lint": "eslint ./src",
    "test:jest": "jest --env=jest-environment-jsdom-global",
    "test:coverage": "jest --env=jest-environment-jsdom-global --coverage",
    "test:bundlesize": "bundlesize"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minivera/lander.git"
  },
  "keywords": [
    "virtual",
    "dom",
    "light"
  ],
  "author": "Minivera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minivera/lander/issues"
  },
  "homepage": "https://github.com/minivera/lander#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-3": "^6.22.0",
    "bundlesize": "^0.17.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "gzip-size": "^4.1.0",
    "jest": "^22.4.3",
    "jest-environment-jsdom": "^23.1.0",
    "jest-environment-jsdom-global": "^1.1.0",
    "parcel": "^1.9.3",
    "pretty-bytes": "^4.0.2",
    "raf": "^3.4.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "uglify-js": "^3.4.2"
  }
}
