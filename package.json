{
  "name": "lander",
  "version": "0.0.1",
  "description": "A VDOM engine made for the modern web",
  "browser": "dist/lander.browser.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsdx build",
    "start": "parcel src/index.html -p 8080",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "prepublishOnly": "npm run build",
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "clean": "git clean -fdX ."
  },
  "main": "dist/index.js",
  "umd:main": "dist/react-vitae.umd.production.js",
  "module": "dist/react-vitae.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minivera/lander.git"
  },
  "keywords": [
    "virtual",
    "dom",
    "light"
  ],
  "author": "Minivera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minivera/lander/issues"
  },
  "homepage": "https://github.com/minivera/lander#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@testing-library/jest-dom": "^4.2.3",
    "@types/jest": "^24.0.22",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "parcel": "^2.0.0-alpha.2.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tsdx": "^0.11.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "dependencies": {}
}
