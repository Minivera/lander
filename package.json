{
  "name": "lander",
  "version": "0.0.1",
  "description": "A VDOM engine made for the modern web",
  "browser": "dist/lander.browser.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production tsdx build",
    "start": "parcel src/index.html --open",
    "lint": "eslint 'src/**/*.{js,ts,jsx,tsx}'",
    "prepublishOnly": "npm run build",
    "test": "tsdx test --env=jsdom",
    "test:coverage": "tsdx test --env=jsdom --coverage",
    "clean": "git clean -fdX ."
  },
  "main": "dist/index.js",
  "umd:main": "dist/react-vitae.umd.production.js",
  "module": "dist/react-vitae.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minivera/lander.git"
  },
  "keywords": [
    "virtual",
    "dom",
    "light"
  ],
  "author": "Minivera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minivera/lander/issues"
  },
  "homepage": "https://github.com/minivera/lander#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@types/jest": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tsdx": "^0.8.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.0.0"
  }
}
